.iamgeRootAll {
  margin-top: 10rpx;
  margin-bottom: 20rpx;
  display: flex;
  display: -webkit-flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
}

.imgfenmian{
  margin: 6rpx;
  position: relative;
  width: 176rpx;
  height: 176rpx;
  background: gainsboro;
}

.imgItem {
  margin: 6rpx;
  position: relative;
  width: 180rpx;
  height: 210rpx;
  background: gainsboro;
}

.img {
  width: 100%;
  height: 100%;
}

.closeImg {
  position: absolute;
  right: 0px;
  width: 32rpx;
  height: 32rpx;
}

.photo {
  width: 50%;
  height: 50%;
  margin: 25%;
}

.qupuvideo{
  width: 300rpx;
  height: 250rpx;
}
